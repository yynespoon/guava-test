<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="person" class="com.test.spring.create.Person">
        <property name="username" value="zhangsan"/>
        <property name="age" value="13"/>
    </bean>

    <bean id="person2" factory-bean="staticFactory" factory-method="getPerson"/>

    <bean id="person3" class="com.test.spring.create.Person" factory-method="getPerson" />

    <bean id="person4" class="com.test.spring.create.PersonFactoryBean" />

    <bean id="person13" class="org.springframework.beans.factory.serviceloader.ServiceLoaderFactoryBean">
        <property name="serviceType" value="org.springframework.beans.factory.FactoryBean"/>
    </bean>

    <bean id="staticFactory" class="com.test.spring.create.PersonStaticFactory"/>

</beans>